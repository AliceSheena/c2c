# Rclone configuration for OneDrive to B2 Migration
# Copy this file to rclone.conf and update with your credentials

[onedrive]
type = onedrive
client_id = your_microsoft_client_id
client_secret = your_microsoft_client_secret
token = your_oauth_token_here
drive_type = personal

[b2]
type = b2ea
account = your_b2_account_id
key = your_b2_application_key
hard_delete = false
versions = false
